<div id="conversations">
  <h2>Conversations</h2>
    <% if @existing_chats_users.length > 0 %>
      <div class="conversation-list">
        <% @existing_chats_users.each do |other_user| %>
          <% if other_user != current_user %>
            <%= render 'users/user', user: other_user  %>
          <% end %>
        <% end %>
      </div>
    <% else %>
      <p>No Active Chats</p>
    <% end %>
</div>

<div id="friends">
  <h2>Friends</h2>
  <div class="friends-list">
    <%= render @other_users %>
  </div>
</div>